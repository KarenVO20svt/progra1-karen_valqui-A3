Proceso Ejercicio4
	Leer gT
	Leer s1
	Leer s2
	Leer s3
	//Restricciones
	tAcc <- s1+s2+s3
	GaS1 <- (gT/tAcc)*s1
	GaS2 <- (gT/tAcc)*s2
	GaS3 <- (gT/tAcc)*s3
	Escribir "La ganancia del s1 es " , GaS1
	Escribir "La ganancia del s2 es " , GaS2
	Escribir "La ganancia del s3 es " , GaS3
